extends layout

block content
 .w3-card-4
  header.w3-container.w3-teal
   h1 Planta #{c._id}
  .w3-container
   ul.w3-ul.w3-card-4(style="width:50%")
    li #[b Identificador: ] #{c._id}
    li #[b Número de Registo: ] #{c.NumRegisto}
    li #[b Código de Rua: ] #{c.codRua} 
    li #[b Rua: ] #{c.Rua}
    li #[b Local: ] #{c.Local}
    li #[b Freguesia: ] #{c.Freguesia} 
    li #[b Espécie: ] #{c.Espécie}
    li #[b Nome Cientifico: ] #{c.NomCientifico}
    li #[b Origem: ] #{c.Origem} 
    li #[b Data de Plantação: ] #{c.DataPlantação}
    li #[b Estado: ] #{c.Estado}
    li #[b Caldeira: ] #{c.Caldeira} 
    li #[b Tutor: ] #{c.Tutor}
    li #[b Implantação: ] #{c.Implantação} 
    li #[b Gestor: ] #{c.Gestor}
    li #[b Data de atualização: ] #{c.DataAtualização}
    li #[b Número de intervenções: ] #{c.NumIntervenções} 
  footer.w3-container.w3-teal
   address Generated by RPCW2023 in #{d} - #[a(href="/") Voltar]
